{
  "name": "sakuga-server",
  "version": "1.0.0",
  "description": "Sakuga backend server",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "dev:all": "concurrently -n server,client -c blue,green \"npm run dev\" \"npm run dev --prefix ../client\"",
    "build": "npm run build --prefix ../client",
    "prod": "npm run build && npm run start"
  },
  "dependencies": {
    "@fal-ai/client": "^1.8.1",
    "@google/generative-ai": "^0.24.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-static-gzip": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.77.0",
    "replicate": "^1.0.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
